    .port_info 0 /OUTPUT 8 "out";
    .port_info 1 /INPUT 1 "enable";
    .port_info 2 /INPUT 1 "clk";
    .port_info 3 /INPUT 1 "reset";
v0x121607830_0 .net "clk", 0 0, v0x12161dea0_0;  1 drivers
v0x12161dc40_0 .net "enable", 0 0, v0x12161df40_0;  1 drivers
v0x12161dce0_0 .var "out", 7 0;
v0x12161dda0_0 .net "reset", 0 0, v0x12161e0c0_0;  1 drivers
E_0x1216095c0 .event posedge, v0x121607830_0;
    .scope S_0x1216076c0;
T_0 ;
    %wait E_0x1216095c0;
    %load/vec4 v0x12161dda0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x12161dce0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x12161dc40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x12161dce0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x12161dce0_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x121608f30;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12161dea0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x121608f30;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x12161dea0_0;
    %inv;
    %store/vec4 v0x12161dea0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x121608f30;
T_3 ;
    %vpi_call 2 13 "$monitor", "time=%0t, en=%0b, rst=%0b, out=%0d", $time, v0x12161df40_0, v0x12161e0c0_0, v0x12161dff0_0 {0 0 0};
    %vpi_call 2 14 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x121608f30;
T_4 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12161df40_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12161e0c0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12161e0c0_0, 0, 1;
    %delay 5000, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
